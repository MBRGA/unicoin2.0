  "source": "// SPDX-License-Identifier: MIT\n\npragma solidity ^0.8.12;\n\nimport \"@opengsn/contracts/src/BaseRelayRecipient.sol\";\nimport \"@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol\";\n\nabstract contract UnicoinBase is BaseRelayRecipient, Initializable {\n\n    address registry;\n\n    function initialize(address _registry) public virtual initializer {\n        registry = _registry;\n    }\n\n    modifier onlyRegistry() {\n        require(_msgSender() == registry, \"Can only be called by registry\");\n        _;\n    }\n\n    function versionRecipient() external virtual override pure returns (string memory) {\n        return \"3.0.0-alpha0+Unicoin\";\n    }\n\n}",
